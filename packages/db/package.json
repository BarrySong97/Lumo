{
  "name": "@lumo/db",
  "version": "0.1.11",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*.ts",
      "types": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "catalog:"
  },
  "devDependencies": {
    "drizzle-kit": "catalog:",
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  }
}
